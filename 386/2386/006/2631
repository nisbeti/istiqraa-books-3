<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span>السمعاني أبو سعد اسمه عبد الكريم بن محمد السمعاني أبو المظفر اسمه عبد الرحيم بن عبد الكريم ابن السمناوي هو أحمد بن محمد التنيسي سمنون المحب <span class="c2">(٣)</span> ٣٧٤ السموئيل بن عادياء اليهودي <span class="c2">(١)</span> ٧٥، ٧٦ <span class="c2">(٢)</span> ٦٧ <span class="c2">(٥)</span> ٤٣١ سمويه <span class="c2">(إسماعيل بن عبد الله العبدي)</span> <span class="c2">(٤)</span> ٢٧٩ السمهري العكلي اللص <span class="c2">(١)</span> ٢٧٤، ٥٢٩ <span class="c2">(٢)</span> ٢٧٦ <span class="c2">(٣)</span> ١٦٩، <span class="c2">(٤)</span> ٤٢، ١٩٧ السميدع بن حوثر <span class="c2">(١)</span> ١٠٥ <span class="c2">(٤)</span> ٢٣١ <span class="c2">(٥)</span> ١٨٥ سميرة امرأة من المهاجرات <span class="c2">(٣)</span> ٢٦٩ ابن سميفع <span class="c2">(٥)</span> ١٧٤ سمية أم زياد <span class="c2">(٣)</span> ١٥٤ أبو سناء القيسي <span class="c2">(٥)</span> ٣٣٩ سنان داعي الإسماعيلية <span class="c2">(٣)</span> ٣٣٤ <span class="c2">(٤)</span> ١٣٧ سنان بن أبي حارثة <span class="c2">(١)</span> ٢٥٣ <span class="c2">(٣)</span> ٢٠٢، ٣٢٦ سنان بن سلمة بن المحبق الهذلي <span class="c2">(١)</span> ٥١٠ <span class="c2">(٤)</span> ٣٥٣ <span class="c2">(٥)</span> ١٩٧ سنان بن علوان فرعون <span class="c2">(٥)</span> ٤٤٢ سنان بن يحيى بن الأدركون <span class="c2">(٤)</span> ٤٠٧ أبو سنان اليماني <span class="c2">(٢)</span> ١٦٩، ١٧٧ <span class="c2">(٥)</span> ٤٤٨ السناوردية <span class="c2">(٣)</span> ٣٦٤ سنجر بن ملكشاه بن ألب أرسلان السلجوقي <span class="c2">(١)</span> ٢٦٠ <span class="c2">(٣)</span> ٢٦٢ <span class="c2">(٤)</span> ٤٨٩ <span class="c2">(٥)</span> ١١٤، ٣٣٢ سنحاريب <span class="c2">(٣)</span> ٣١٢ سندباذ بن بشتاسف بن الهراسف <span class="c2">(١)</span> ٢٤٦ السندي بن عبدويه الدهكي <span class="c2">(٢)</span> ٤٩٣ سنقر أتابك <span class="c2">(٢)</span> ١٠٧ سنقر كنجك الأمير <span class="c2">(١)</span> ١٧٦ سنمار البناء <span class="c2">(٢)</span> ٤٠١ <span class="c2">(٣)</span> ٣١٩ ابن السني <span class="c2">(٥)</span> ٢٢٢ ابن أبي سنة اسمه ابراهيم أبو سراج الضبي <span class="c2">(١)</span> ٤٩٣ سواد بن قارب <span class="c2">(٣)</span> ٣٣٢ سوار أبو الحلي الزجاجي <span class="c2">(٣)</span> ١٣٣ سوار بن عبد الله <span class="c2">(١)</span> ٢٧٢ <span class="c2">(٣)</span> ٢٨١ سوار بن المضرب المازني <span class="c2">(٢)</span> ٨٤ <span class="c2">(٣)</span> ٣٦٧ سوار بن همام العبدي <span class="c2">(٣)</span> ١١٣ سوار بن يزيد بن عدي بن زيد العبادي <span class="c2">(٣)</span> ٢٧٦ ابن سوار أبو طاهر <span class="c2">(٢)</span> ١٩٢ السودان <span class="c2">(١)</span> ٢٤٠، ٢٧٨، ٣٢١ <span class="c2">(٢)</span> ١٢ سودان بن يوسف <span class="c2">(١)</span> ٥٢٦ سودة بنت عمير بن هذيل <span class="c2">(٥)</span> ٥٧ سوراء بنت أردوان بن باطي <span class="c2">(٣)</span> ٢٧٨ سورة بن الحسن الآلوزاني <span class="c2">(١)</span> ٥٦ سورة بن شداد أبو الحسن الجنوجردي <span class="c2">(٢)</span> ١٧٣ أبو سورة بن قايد هميم البلخي الفوري <span class="c2">(٤)</span> ٢٧٩ السوريانيون <span class="c2">(٣)</span> ٢٧٨، ٢٧٩<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>