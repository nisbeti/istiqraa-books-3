<p><span class="anchor" id="p3"></span><span class="c5">بهق:</span> ٨٥ بون: بواني ٣٧٩ بيض: ٥٥، ٧٥، ٧٠، ٢٦٥، ٣٠١.<a href="#p3" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p4"></span><span class="c2">(ت)</span> تر: أترها ٤٩٦-<span class="c5"> ٤٦٠ تفل:</span> تتفل ٢٤٢-<span class="c5"> ٢٤٠ تمر:</span> تمير ٣٢٩ تملك: تامكة ٥٨٤-<span class="c5"> ٥٤٨ تم:</span> تمام ٧٨،<span class="c5"> تميم ٣٢١ توأم:</span> ٨٠ <span class="c2">(ث)</span> ثبت: فأثبته ٢٠ ثبج: الثبج كتثبيج ٣٧١، ٣٤٧، أثباج ٥٣٧-<span class="c5"> ٥٠٦ ثرد:</span> ثردة ١٣٨ ثرم: أثرم ٤٩٧، الثرم ٥٢٩،<span class="c5"> ٤٩٨ ثط:</span> اثطاط ٥٥٣،<span class="c5"> ٥١٩ ثعب:</span> التعلبية ٢٢٩ مثعبا ٥٤٣ ٥١٠ ثفر: مثفار ١٦٠، ١٦١،<span class="c5"> الثفر ٢٧٧ ثفل:</span> ثفال ٢٦١ ثنى: تثنى ٢٤٣،<span class="c5"> الثنيان ٣٠٦ ثيل:</span> بثيل ٤٧١، كثيل ٤٧٢ <span class="c2">(ج)</span> جبب: جباء ٥٩٥ جبر: جبور ٣٥٢ جدع: أجدع ٣٤١ جدل: الجديلا ٣٤٥ جذر: جذير ٣٧٣ جذع: جذعة ٤٦٥ جذم: أجذم مجذام ٢٦١ جرد: الجردة ٣٢٦ جرجرا ٤٨٠ مجر ٢٨٨ جرم: جرام ٢٩٠ جرمز: جراميزه ٢٤٥،<span class="c5"> ٣٣٦ جزر:</span> جزرا،<span class="c5"> جازر ٣٣٢ جش:</span> أجش ٢١٥ أجشة ٣٥٨ جعد: جعاد ٤١٥ جعر: جاعرتيه ٤٨٨ جفر: جفرته ٢٩٢ جلب: جلب ٣٧٩ جلجل: يتجلجل ٢٣٥ الجلاجل ٤١٥<a href="#p4" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>