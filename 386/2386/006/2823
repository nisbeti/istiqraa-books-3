الي محمد بن الملك المظفر <span class="c2">(١)</span> ٢٤٠ <span class="c2">(٢)</span> ٣٠٠، ٤٤٥ الملكانية <span class="c2">(٢)</span> ٥٢٨، ٥٣٧ ملكشاه جلال الدولة بن ألب أرسلان <span class="c2">(١)</span> ٢٦٩ <span class="c2">(٢)</span> ١٤١ <span class="c2">(٣)</span> ٣١٦ <span class="c2">(٥)</span> ٢٠١ مليح الهذلي <span class="c2">(١)</span> ٢٢٣ <span class="c2">(٢)</span> ٥٥، ١٧٣، ٣٢٦ <span class="c2">(٣)</span> ٩٣، ١٦٣، ٣٣٠ <span class="c2">(٤)</span> ٢٢، ٩٤، ٢٦٩، ٣٨٠ مليكة زوجة أبي بكر <span class="c2">(٣)</span> ٢٦٥ ابن أبي مليكة <span class="c2">(عبد الله بن عبيد الله)</span> <span class="c2">(٤)</span> ١٠٥ ممجه هو أحمد بن محمد الجيراني <span class="c2">(٢)</span> ١٩٧ الممزق اسمه شماس بن نهار <span class="c2">(٤)</span> ١٥٠ مناد بن بلكين <span class="c2">(٣)</span> ٣٩١ ابن المنادي اسمه أحمد بن جعفر بن محمد ابن مناذر اسمه محمد بن مناذر منبه بن حبيب <span class="c2">(٥)</span> ٢٢٣ منبه بن محمد بن أحمد أبو وهب الفرواني <span class="c2">(٢)</span> ١٩٣ <span class="c2">(٤)</span> ٢٥٧ المتنخل الهذلي <span class="c2">(مالك بن عويمر)</span> <span class="c2">(١)</span> ١٠١، ٣٩٠ <span class="c2">(٥)</span> ٢٩٢ المنتصر الخليفة <span class="c2">(٣)</span> ١٧٣<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>