<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span>الأحوص هو زيد بن عمرو بن قيس بن عتاب بن كلومي <span class="c2">(٤)</span> ٢٣ الأحوص بن جعفر بن كلاب <span class="c2">(٢)</span> ٣٦٦ <span class="c2">(٣)</span> ٣٦ الأحوص <span class="c2">(عبد الله)</span> بن محمد الأنصاري <span class="c2">(١)</span> ٣٥٩، ٣٩٣ <span class="c2">(٢)</span> ١٠١، ٣٣٥، ٣٣٦ <span class="c2">(٣)</span> ٢٩٧ <span class="c2">(٤)</span> ١٥١، ٢١٨، ٣١٦، ٤٤٠ <span class="c2">(٥)</span> ٢٤، ١٢٠، ١٨٠، ٢٩٣ أبو الأحوص الرياحي <span class="c2">(٥)</span> ٢٦١ الأحول الكندي <span class="c2">(٤)</span> ٥٢ الأحول بن نجاح <span class="c2">(٢)</span> ١٠٦، ٤١٤ أحيحة بن الجلاح <span class="c2">(١)</span> ٢٩٢ <span class="c2">(٢)</span> ٣٢، ١٥٩، ٣١١ <span class="c2">(٣)</span> ١٥٥، ٤٥٤ <span class="c2">(٤)</span> ٤٠٠ أحيد بن الحسين بن علي أبو أحمد السلمي البامياني <span class="c2">(١)</span> ٣٣٠ أحيد بن الحكم بن خداش أبو سعيد الباركثي <span class="c2">(١)</span> ٣٢٠ أحيد بن لقمان الشوائي <span class="c2">(٣)</span> ٣٦٩ الأحيمر السعدي <span class="c2">(١)</span> ٦٢، ٦٦، ٨٦ <span class="c2">(٢)</span> ١٨٧، ٤٨٢ <span class="c2">(٥)</span> ١٦٥ الأخزر بن ثابت <span class="c2">(٢)</span> ١٥١ الأخزر بن يزيد القشيري <span class="c2">(٣)</span> ٨٧ الأخشيذ <span class="c2">(محمد بن طغج)</span> <span class="c2">(١)</span> ٧٠ <span class="c2">(٥)</span> ٤٧ الأخضر بن هبيرة بن عمرو بن ضرار الضبي <span class="c2">(٢)</span> ١١٣ الأخطل <span class="c2">(غياث بن الغوث)</span> <span class="c2">(١)</span> ٧٨، ١٦٧، ١٦٨، ١٩٩، ٢٢٠، ٢٤٥، ٣٦٦، ٣٨٦، ٣٩٤، ٤٢٤، ٤٢٧، ٤٩٣، ٤٩٨، ٥٣١ <span class="c2">(٢)</span> ٨٢، ٩٠، ١١٣، ٢٠٤، ٢٠٧، ٢٥٢، ٢٥٦، ٢٦٢، ٢٧٤، ٢٩٠، ٣٣٤، ٣٤٥، ٤١٥، ٤٨٩، ٤٩٤، ٤٩٨، ٥٣٠ <span class="c2">(٣)</span> ٣٧، ٨٩، ٩٣، ٩٥، ١٠٦، ١٢٣، ١٢٥، ٢١١، ٢٣٠، ٢٤٢، ٢٩٢، ٣٣٥، ٣٥٣، ٤٠٥، ٤٣٤ <span class="c2">(٤)</span> ٦٥، ٩٣، ١٠٤، ١٠٦، ١٦٧، ٢٠٣، ٢٢١، ٢٤٣، ٣٦٦، ٣٧٢، ٤٧٣، ٤٩٤ <span class="c2">(٥)</span> ١١، ٤٣، ٦٣، ١٢٥، ١٥٤، ١٩١، ٢٦٥، ٢٨٤، ٣٤٨، ٣٥١، ٣٧٩، ٤٠٦ الأخفش اسمه سعيد بن مسعدة الأخنس بن شهاب <span class="c2">(٢)</span> ٢٤٦، ٤١٠ <span class="c2">(٣)</span> ٤٦ <span class="c2">(٤)</span> ٣٦٨ أخنوخ بن يرد بن مهلائيل <span class="c2">(٥)</span> ٣٦٧، ٤٠٢ ابن الأخيضر <span class="c2">(٤)</span> ٣١٨ الأدركون الدمشقي <span class="c2">(٤)</span> ٤٠٦ إدريس النبي <span class="c2">(١)</span> ٣١١ إدريس بن إدريس <span class="c2">(٤)</span> ٢٣٠ إدريس بن جعفر الطيار <span class="c2">(٤)</span> ١٨ إدريس بن ظهر بن حكيم أبو محمد القطيعي <span class="c2">(٤)</span> ٣٧٦ إدريس بن عبد الله بن الحسن بن الحسن <span class="c2">(٥)</span> ٣٨٤ إدريس بن عبد العزيز الورداني <span class="c2">(٥)</span> ٣٧١ إدريس بن علي بن إدريس أبو الفتح البياري<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>