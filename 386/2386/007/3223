<p><span class="anchor" id="p2"></span>- أ- الأبيوردي <span class="c2">(١)</span> ٤١٤ بستان إبراهيم <span class="c2">(٤)</span> ١٦٤ عوارض أحمد بن الحسن <span class="c2">(١)</span> ٢٠١ أشناذجرد أحمد بن علي <span class="c2">(٥)</span> ٢٤٩ نابل أحمد بن فارس <span class="c2">(١)</span> ٨١ أبو قبيس، ٢٨١ الأوطاس أحمد بن محمد الخارزنجي أحمد بن يحيى ثعلب الأديبي <span class="c2">(٤)</span> ٥٧ الظّباء، ١٦٨ عوس، ٣١٠ قتائدة إسحاق بن مرار أبو عمرو الشّيباني إسماعيل بن القاسم القالي الأزهري <span class="c2">(محمد بن أحمد)</span> <span class="c2">(٢)</span> ٧٤ تجر، ٨٣ الثّلم <span class="c2">(٤)</span> ٣٣ طريثيث، ١٦٩ عوقة، ٢٧٩ الفوارس الأصمعي <span class="c2">(عبد الملك بن قريب)</span> <span class="c2">(١)</span> ٢٤٠ أكلب، <span class="c2">(٢)</span> ٢١٣ حبس، ٢٦٧ حصير <span class="c2">(٣)</span> ٩ الذّهلول، ٢٧ رجام، ٥٩ الرقة، ١٣٤ زخّة، ٢٤٦ سماهيج، ٢٧٦ السّواس <span class="c2">(٤)</span> ٤٠ طمار، ٨٤ العثانة، ١٨٩ الغرّاء، ٢٤٦ فرتاج، ٣٢٠ قربق، ٣٣٣ قرن <span class="c2">(٥)</span> ١١٧ مريخ، ١٩٩ منى، ١٥١ مطلوب، ٤٥٢ ينوف ابن الأعرابيّ <span class="c2">(محمد بن زياد)</span> <span class="c2">(١)</span> ٦١ أباض، ٢٥٣ أمرّ، ٢٥٧ الأنبار، ٤٤٨ بطنان <span class="c2">(٢)</span> ٤٦ تمر، ١٥٣ الجلس، ٢٢٣ حجر، ٢٥٥ حزن يربوع، ٢٩٦ حليمات، ٣٠١ الحمائر، ٣٣٥ الخابور، ٣٥٧ خرج هجين، ٤١٧ دابق، ٤٢٤ دارأت العرب <span class="c2">(٣)</span> ١٠ ذيالة، ٩٤ روضة ماويّة، ٣٥٢ شغب <span class="c2">(٤)</span> ٢١ طثرة، ٩٣ العراق، ١٢٢ عسكر أبي جعفر، ١٣٣ العفيف، ١٥٧ عمّ، ٢٠٦ غضبان، ٢٧٠ فلّا <span class="c2">(٥)</span> ١٠ لبب، ٤٨ ماويّة، ٥٦ المجازة، ١٠٤ مرّ، ٤٥٠ ينخوب<a href="#p2" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p3"></span>ب أبو بكر ابن دريد<a href="#p3" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>