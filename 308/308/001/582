<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span><span class="c5">رذم:</span> رذم الخيشوم ٣٥٢ رز: الرز ٤١٩ رسح: بأرسح ٥٨٣ رسن: المرسن ٤٦٦ رشق: المرشقات ٥٣٠ ٤٩٩ رعل: الرعال ٣٥٦،<span class="c5"> رعلاء ٤١٩ رغث:</span> رغوث ٣٢٥ رغم: الرغام ٤٦٥ رفض: ارفضاضها ٣٤٨ رقط: أرقط ١١٢، ١١٨،<span class="c5"> الرقط:</span> ١١٤، ١١٥،<span class="c5"> رقع:</span> مرقع ١١٨ رقل: أرقل ٢٥٤ رمص: رمص ٣٤٦،<span class="c5"> ٤١٥ رمك:</span> الرمكة ٣٢٥،<span class="c5"> رمكاء ١٣٨ رما:</span> أرمى ٣٥٥ رهط: أراهط ٣٥٣ روب: مرواب ٤١٣ روح: استرواح ٣٥٤ روح ٣٥٩ <span class="c2">(ز)</span> زب: أزب ٣٤٩،<span class="c5"> ٤٦٤ زبل:</span> الزبل ١١٧ زجل: زجل ٢٢٧ زغب: مزغب ٢٣٥ زعب: زاعبي ٤٦٧ زل: تزل: ٥١٣ زمج: مزمهج ٣٤٧ زمن: يزمنهم ٣٦٦ زنا: زنت ١١٧ زوج: المزدوج ١٣٩ زود: الأزواد ٥١٩ مستزاد ٣٥٨ زور: أزور ٣٤١ زيف: فتزيف ٢٢١ <span class="c2">(س)</span> سأد: أسأدت ٢٣١ سام أبرص:<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p2"></span><span class="c5">١٤٣ سربخ:</span> مسربخ ٢٣١ سدى: تسديتها ٣٩٧ سطع: ساطع ٢٧٥ سعل: سعلت ١١٧ سقط: سقط ٣٣٣ سقى: ٤٠٧ ٣٨٩ سلأ: سلأوا ١٦٥ سلب: أسلوب ٤٦٥ سلجم: سلجم ٣٩٩ سلع: الأسلع ١٠٢، ١٠٣،<span class="c5"> ١٢٥ سلك:</span> سلكى ٥٢٧ سلم: السليم ١٢١ سمير: سمير ٥٢٣<a href="#p2" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>