<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span>٣٢ تصلب، ٢٧٣ حضير، ٤٧٨ دنن <span class="c2">(٣)</span> ١١٠ ريّان، ٤٠٨ الصّعيراء <span class="c2">(٤)</span> ١٣٨ العقوبان، ٤٣٤ كبشات، ٤٥١ الكرس <span class="c2">(٥)</span> ٢٣٥ مهزول، ٢٩٢ النّطوف- س- أبو سعد <span class="c2">(٥)</span> ٢٢٨ المولة السكّري <span class="c2">(الحسن بن الحسين)</span> <span class="c2">(٢)</span> ٣٧٩ خفّان السكونيّ <span class="c2">(عمر بن محمد)</span> <span class="c2">(٥)</span> ٢٧٥ نخب ابن السكّيت <span class="c2">(يعقوب بن إسحاق)</span> <span class="c2">(١)</span> ٢٤٩ أمّ أوعال، ٢٥١ أم خرمان، ٢٥٤ أمّ غرس <span class="c2">(٢)</span> ١٢٤ الجرد، ١٥٢ الجلس <span class="c2">(٤)</span> ٣٦٧ القصيم سيبويه <span class="c2">(عمرو بن عثمان)</span> <span class="c2">(١)</span> ٤٨١ بلد <span class="c2">(٤)</span> ٣٩٣ قلهى السّيرافي <span class="c2">(يوسف بن الحسن (٣)</span> ٣٥٥ شقرة<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p2"></span>ش أبو الشعشاع الناجي <span class="c2">(٢)</span> ٣٦١ الخرماء شمر <span class="c2">(بن حمدويه)</span> <span class="c2">(٢)</span> ١٦٧ جنان- ع- العامري <span class="c2">(٣)</span> ٢٧ رجام ابن عبّاس <span class="c2">(لعلّه عبد الله)</span> <span class="c2">(٤)</span> ٣٣٧ العباس بن الفرج الرياشي عبد الله بن عبد العزيز أبو عبيد عبد الله بن عثمان أبو بكر الصّدّيق عبد الله بن مسلم ابن قتيبة عبد الملك بن قريب الأصمعيّ أبو عبيد <span class="c2">(عبد الله بن عبد العزيز)</span> <span class="c2">(٢)</span> ١٥٧ الجلهمتان <span class="c2">(٤)</span> ٣٧٣ قطر أبو عبيدة <span class="c2">(معمر بن المثنّى)</span> <span class="c2">(٢)</span> ٢١٠ حائل <span class="c2">(٤)</span> ٢٦٩ الفقير <span class="c2">(٥)</span> ١٨١ مكّة عثمان بن جنّي ابن جنّي عرّام بن الأصبغ <span class="c2">(٤)</span> ١٩١ غران علي بن الحسين أبو عبيد أبو عليّ الفارسيّ <span class="c2">(الحسن بن أحمد)</span> <span class="c2">(٣)</span> ٤٣٥ صهاب <span class="c2">(٤)</span> ٣٨٥ قلاب أبو عليّ الفسويّ <span class="c2">(١)</span> ٥٣٦ بينونة عمر بن محمد السكوني العمرانيّ <span class="c2">(١)</span> ٤٥٢ بعال <span class="c2">(٢)</span> ٤٠٩ خويّ <span class="c2">(٣)</span> ١٦٨ سابور، ١٦٨ ساتيدما، ٢٥١ سمعان <span class="c2">(٤)</span> ٣٩٤ القليب، ٤١٨ القهز أبو عمرو الشّيباني <span class="c2">(إسحاق بن مرار)</span> <span class="c2">(٢)</span> ٢٩٧ حليّة، ٤٢٩ دارة قرح <span class="c2">(٣)</span> ٤٢٣ صماد عمرو بن عثمان سيبويه عمرو بن كلاب <span class="c2">(١)</span> ٣٤١ بحار<a href="#p2" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
