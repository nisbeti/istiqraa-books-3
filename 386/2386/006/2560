<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span>جاوشيغر <span class="c2">(٢)</span> ٣٦٨ الجبائي <span class="c2">(٥)</span> ١٧٢ جباخ بن الحسين بن يوسف أبو بكر الصوفي الكنكوري <span class="c2">(٥)</span> ٤٨٤ جبار بن مالك بن حماد الشمحي الفزاري <span class="c2">(١)</span> ٥٩ جبارة بن مغلس <span class="c2">(١)</span> ٥٢٦ ابن الجبان اسمه عبد الوهاب بن عبد الله جبر بن رافع البكري <span class="c2">(٣)</span> ٢٢٣ جبرئيل بن ناشرة المعافري <span class="c2">(٤)</span> ٢٦٣ جبلة المقري <span class="c2">(٥)</span> ١٧٣ جبلة بن الأيهم <span class="c2">(٢)</span> ٨٠ جبلة بن جريس الحلابي <span class="c2">(٣)</span> ٩٠ جبلة بن الحارث <span class="c2">(١)</span> ٣٩٢ جبلة بن مالك بن كلثوم بن شيماء <span class="c2">(٥)</span> ٢٢٧ أبو جبلة <span class="c2">(٥)</span> ٢٢٣ جبير الموتفكي <span class="c2">(١)</span> ١٨٥ جبير بن الحسن اليمامي <span class="c2">(٥)</span> ٤٤٧ جبير بن الحويرث <span class="c2">(٤)</span> ٣٤١ جبير بن حية <span class="c2">(١)</span> ٤٣٥ جبير بن مطعم بن عدي بن نوفل <span class="c2">(٢)</span> ٤٢٢ <span class="c2">(٣)</span> ١٠١ جبير بن نفير <span class="c2">(٢)</span> ١٢٧ أبو جبيلة <span class="c2">(١)</span> ٣٩٥ <span class="c2">(٢)</span> ٢٤٢ <span class="c2">(٥)</span> ٨٥ جبيهاء اسمه يزيد بن عبيد جثامة <span class="c2">(٢)</span> ٥١٥ جثامة بن عمرو بن محلم الشيباني <span class="c2">(٤)</span> ٢٢٠ الجحاف بن حكيم السلمي <span class="c2">(١)</span> ٤٢٧ <span class="c2">(٣)</span> ٣٧ <span class="c2">(٤)</span> ٤٥٥ جحدر بن معاوية المحرزي اللص <span class="c2">(١)</span> ٤٠٨، ٥٣٠ <span class="c2">(٢)</span> ٤٦، ٨٨، ١٩٠، ٢٢٢، ٤٧٩، ٥٤٤ <span class="c2">(٣)</span> ٣٧٩ <span class="c2">(٤)</span> ١٠٧ <span class="c2">(٥)</span> ١٣ جحظة البرمكي اسمه أحمد بن جعفر جخجخ النحوي اسمه عبيد الله بن أحمد جدار بن بكر الدبيلي <span class="c2">(٢)</span> ٤٣٩ أبو جدعاء الطهوي <span class="c2">(٥)</span> ٥١ ذو جدن <span class="c2">(علقمة بن شراحيل الحميري)</span> <span class="c2">(١)</span> ٣٦٤، ٥٣٥ <span class="c2">(٢)</span> ٤٣ <span class="c2">(٣)</span> ٢٣٥ <span class="c2">(٤)</span> ٢١٠ جدة بن حزم بن ربان بن حلوان <span class="c2">(٢)</span> ١١٤ جدي بن الدلهاث بن عشر بن حلوان القضاعي <span class="c2">(٢)</span> ١٣٥، ٢٦٨ جديلة بن المشمخر الجديسي <span class="c2">(٥)</span> ٤٤٤ جذع الغساني <span class="c2">(٢)</span> ٢٩٦ جذيمة الأبرش بن مالك بن فهم <span class="c2">(١)</span> ٤٧٣ <span class="c2">(٢)</span> ٣٣٠ <span class="c2">(٣)</span> ١٢٩ <span class="c2">(٤)</span> ١٢٨ ابن الجراح <span class="c2">(٥)</span> ٣٢٦ جران العود النميري <span class="c2">(١)</span> ١٩٥ <span class="c2">(٢)</span> ٣٩٠، ٤٢٧ <span class="c2">(٣)</span> ٩٠، ٢٣٣ <span class="c2">(٤)</span> ١١٤، ١٩٢، ٣٤٥، ٣٤٦، ٤٢٧ الجرباء بنت عقيل <span class="c2">(٢)</span> ٥١٥ ابن جرجان <span class="c2">(٥)</span> ٢٢٢ الجرجاني أبو عبد الله <span class="c2">(١)</span> ٢٩٩، ٥٢٩<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>