ن <span class="c2">(٥)</span> ٣٢٣ عثمان بن مظعون <span class="c2">(٤)</span> ٤٧١ عثمان بن معلي الباشمناني <span class="c2">(١)</span> ٣٢٤ عثمان بن أبي نصر أبو عمرو الحفصاباذي <span class="c2">(٢)</span> ٢٧٦ عثمان بن أبي نصر بن منصور أبو الفتوح المسعودي <span class="c2">(٥)</span> ١٢٦ عثمان بن نهيك <span class="c2">(٢)</span> ٤٢٢ <span class="c2">(٣)</span> ٢٦ أبو عثمان المازني انظر في المازني أبو عثمان الناجم <span class="c2">(٢)</span> ٥٠٨ العجاج <span class="c2">(١)</span> ٦٦، ٩٦، ٢٤٩ <span class="c2">(٢)</span> ٣١٩ <span class="c2">(٣)</span> ١٠٧، ٣٢١، ٤٥١، ٤٦٣ <span class="c2">(٤)</span> ٢٤، ٢٧١، ٢٧٦، ٣٦٥، ٤٠٣ <span class="c2">(٥)</span> ٣٦٩ ابن أبي العجائز <span class="c2">(١)</span> ٥٢٠ <span class="c2">(٢)</span> ٩٦، ٤٣٣، ٤٩٦، ٥٢٩، ٥٤٣ <span class="c2">(٣)</span> ٢٢٠، ٢٢٩، ٣٢١، ٤١٤ <span class="c2">(٤)</span> ٤١٧ <span class="c2">(٥)</span> ٢٤١، ٤٤١ ١٤ فهرس معجم البلدان<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>