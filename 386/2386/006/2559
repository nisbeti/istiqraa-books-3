 <span class="c2">(٢)</span> ٥٦ أبو جابر الأسدي <span class="c2">(٤)</span> ١٨٢ أبو جابر الكلابي <span class="c2">(١)</span> ٢٨٠ <span class="c2">(٤)</span> ٤٣٧ الجاحظ <span class="c2">(عمرو بن بحر)</span> صاحب كتاب اللصوص <span class="c2">(١)</span> ٤٣٩، ٤٦١ <span class="c2">(٢)</span> ٤٠٨، ٤٦٧، ٥٢٣ <span class="c2">(٣)</span> ٥٢ <span class="c2">(٤)</span> ٤٣٣ <span class="c2">(٥)</span> ٤٤١ جار الله هو محمود بن عمر الزمخشري الجارود أبو سويد <span class="c2">(٤)</span> ٥ جارية بن مشمث بن حميري بن ربيعة بن زهرة <span class="c2">(٢)</span> ٢٥٧ جالوت <span class="c2">(١)</span> ٣٦٨ <span class="c2">(٣)</span> ٤٤، ٣٧٦ <span class="c2">(٤)</span> ١٥١، ١٧٧ جالينوس اليوناني <span class="c2">(١)</span> ٥٤ <span class="c2">(٢)</span> ٤٥ الجالينوس صاحب جيش الفرس <span class="c2">(١)</span> ٣٢٧ جامع بن عمرو بن مرخية <span class="c2">(١)</span> ٥٢، ٢٩٤ <span class="c2">(٣)</span> ٤٦٢ <span class="c2">(٤)</span> ١٢٠، ١٥٩ ابن جامع أبو الحسن الصيدلاني <span class="c2">(٣)</span> ٣٩٩<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>