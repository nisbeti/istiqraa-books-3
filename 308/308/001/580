<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span><span class="c5">حم:</span> التحميم ٩٠،<span class="c5"> حمان ٩٠ أحم أحم القلب:</span> ٢٧٢ اللثاء ٣٠٢ أحم المقلتين: ٢٤٦ حمى: تحاماه ٢٨٦،<span class="c5"> الحامي ٤١٩ حنب:</span> تحنيب ٢٦٣ حنف: الحنف،<span class="c5"> أحنف ٣٤١ حوب:</span> فتحوبي ٢٧٠ حور: حواريون ٥٨٥ حول: الحويلا ٣٤٥ لاحتيال: ٢٨٩ حوى: أحوى ٣٤٧ حير: مستحيرة ٢٧٩ <span class="c2">(خ)</span> خبط: يخبطون ٢٥٩ خبا: أخبى ٢٩٧ خبر: يستخبر ٣٠٥ خدج: التخادج ٣٣٣ ٣٢٣ خد: تخدد ٣٧٢ خدر: أخدرى ٢٢٩ خذف: ٥٢٥،<span class="c5"> ٥٢٥ خذي أخذي ٣٤٣ خرج:</span> الخرج ١٧٣ بخارجى ٣٢١ خرص: خرصانها ٥٣٣ ٥٠٢ خرطم: خرطوم ٢٥٤، ٤٦٦، خراطيم ١٦٥،<span class="c5"> ٤٧٤ خرفج:</span> مخرفج ١٦٠،<span class="c5"> ٢٦٩ خرق:</span> ٢٥١ متخرق خروق ٢٨٧ الخرو: ١١٧ خزل: تخزلها ٢٢٣ خسف: الخسف ٢٣١ الخسيف ٢٣١ خشع: متخشعا ٢٧٢ خشم: الأخشم ٤٧٧ خطر: المخاطر ٥٢٦ خظى: خاظيات ٢٦٥ خفت: خفاتا ٤٢١ ٤٠٢ خفق: خفاق ٢٧٦ خلج: مخلجا: ٢٣٧ خلجت، الخيج،<span class="c5"> مخلوجة ٥٦١ ٥٢٦ خلع:</span> يتخلع ٢١٥، التخلع ٢٣٢،<span class="c5"> تخلع ٣٢٨ خلف:</span> خل ٢٩٢،<span class="c5"> الخلاة ١١٧ خمر:</span> تخمر ٤٦٠ فخمر ٤٣٧ خمس: المخمس: ١٣٩ خمص: الأخمص خمع: الخماع ٢٩٨، ٢٤٦،<span class="c5"> خماعها ٢٥٠ لخماع ٢١٧ خنب:</span> الخنب: ٤٦٨ خنس: خنابس ٤١٦ خنز: خنزوانة ٤٦٥ خنق: مخنق ٣٣٢ خور: الخور<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>